	    <entry name="NONE" value="0"/>
	    <entry name="NEAR" value="1"/>
	    <entry name="RECOVERY" value="2"/>
	  </enum>
	  <enum name="FMS_STATE">
	    <entry name="IDLE" value="0"/>
	    <entry name="TAKEOFF" value="1"/>
	    <entry name="CLIMB" value="2"/>
	    <entry name="CRUISE" value="3"/>
	    <entry name="APPROACH" value="4"/>
	    <entry name="LAND" value="5"/>
	  </enum>
	</enums>
	<messages>
	  <message id="42000" name="HEARTBEAT_ICAROUS">
	    <description>ICAROUS heartbeat</description>
	    <field type="uint8_t" name="status" enum="FMS_STATE">See the FMS_STATE enum.</field>
	  </message>
	  <message id="42001" name="KINEMATIC_BANDS">
	    <description>Kinematic multi bands (track) output from Daidalus</description>
	    <field type="int8_t" name="numBands">Number of track bands</field>
	    <field type="uint8_t" name="type1" enum="TRACK_BAND_TYPES">See the TRACK_BAND_TYPES enum.</field>
	    <field type="float" name="min1">min angle (degrees)</field>
	    <field type="float" name="max1">max angle (degrees)</field> 
	    <field type="uint8_t" name="type2" enum="TRACK_BAND_TYPES">See the TRACK_BAND_TYPES enum.</field>
